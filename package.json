{
  "name": "spectra",
  "version": "0.1.0",
  "description": "Fast, lightweight and fully typesafe web framework",
  "main": "./dist/index.cjs",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" \"examples/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\"",
    "build": "tsup",
    "example:node": "tsx ./examples/node.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metebykl/spectra.git"
  },
  "keywords": [
    "web",
    "http",
    "framework",
    "router"
  ],
  "author": "metebykl",
  "license": "MIT",
  "homepage": "https://github.com/metebykl/spectra#readme",
  "devDependencies": {
    "@types/node": "^22.7.9",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
