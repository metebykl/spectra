{
  "name": "@spectrajs/core",
  "version": "0.14.0",
  "description": "Fast, lightweight and fully typesafe web framework",
  "main": "./dist/index.cjs",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metebykl/spectra.git"
  },
  "keywords": [
    "web",
    "http",
    "framework",
    "router"
  ],
  "author": "metebykl",
  "license": "MIT",
  "homepage": "https://github.com/metebykl/spectra#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./validator": {
      "types": "./dist/validator.d.ts",
      "import": "./dist/validator.js",
      "require": "./dist/validator.cjs"
    },
    "./cookie": {
      "types": "./dist/cookie.d.ts",
      "import": "./dist/cookie.js",
      "require": "./dist/cookie.cjs"
    },
    "./dev": {
      "types": "./dist/helpers/dev/index.d.ts",
      "import": "./dist/helpers/dev/index.js",
      "require": "./dist/helpers/dev/index.cjs"
    },
    "./stream": {
      "types": "./dist/helpers/stream/index.d.ts",
      "import": "./dist/helpers/stream/index.js",
      "require": "./dist/helpers/stream/index.cjs"
    },
    "./body-limit": {
      "types": "./dist/middleware/body-limit/index.d.ts",
      "import": "./dist/middleware/body-limit/index.js",
      "require": "./dist/middleware/body-limit/index.cjs"
    },
    "./compression": {
      "types": "./dist/middleware/compression/index.d.ts",
      "import": "./dist/middleware/compression/index.js",
      "require": "./dist/middleware/compression/index.cjs"
    },
    "./cors": {
      "types": "./dist/middleware/cors/index.d.ts",
      "import": "./dist/middleware/cors/index.js",
      "require": "./dist/middleware/cors/index.cjs"
    },
    "./etag": {
      "types": "./dist/middleware/etag/index.d.ts",
      "import": "./dist/middleware/etag/index.js",
      "require": "./dist/middleware/etag/index.cjs"
    },
    "./logger": {
      "types": "./dist/middleware/logger/index.d.ts",
      "import": "./dist/middleware/logger/index.js",
      "require": "./dist/middleware/logger/index.cjs"
    },
    "./powered-by": {
      "types": "./dist/middleware/powered-by/index.d.ts",
      "import": "./dist/middleware/powered-by/index.js",
      "require": "./dist/middleware/powered-by/index.cjs"
    },
    "./request-id": {
      "types": "./dist/middleware/request-id/index.d.ts",
      "import": "./dist/middleware/request-id/index.js",
      "require": "./dist/middleware/request-id/index.cjs"
    },
    "./timeout": {
      "types": "./dist/middleware/timeout/index.d.ts",
      "import": "./dist/middleware/timeout/index.js",
      "require": "./dist/middleware/timeout/index.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "validator": [
        "./dist/validator.d.ts"
      ],
      "cookie": [
        "./dist/cookie.d.ts"
      ],
      "dev": [
        "./dist/helpers/dev/index.d.ts"
      ],
      "stream": [
        "./dist/helpers/stream/index.d.ts"
      ],
      "body-limit": [
        "./dist/middleware/body-limit/index.d.ts"
      ],
      "compression": [
        "./dist/middleware/compression/index.d.ts"
      ],
      "cors": [
        "./dist/middleware/cors/index.d.ts"
      ],
      "etag": [
        "./dist/middleware/etag/index.d.ts"
      ],
      "logger": [
        "./dist/middleware/logger/index.d.ts"
      ],
      "powered-by": [
        "./dist/middleware/powered-by/index.d.ts"
      ],
      "request-id": [
        "./dist/middleware/request-id/index.d.ts"
      ],
      "timeout": [
        "./dist/middleware/timeout/index.d.ts"
      ],
      "utils/*": [
        "./dist/utils/*.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.2",
    "esbuild": "^0.25.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
